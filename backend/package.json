    "jsonfile": "^6.1.0",
    "mysql2": "^3.11.3",
    "process": "^0.11.10",
    "sequelize": "^6.37.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "eslint": "^9.12.0",
    "eslint-plugin-n": "^17.11.1",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  }
