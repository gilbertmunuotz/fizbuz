{
  "name": "fizbuz",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch ./ --watch server.ts --exec \"node --loader ts-node/esm --experimental-specifier-resolution=node server.ts\" --verbose",
    "start": ""
  },
  "nodemonconfig": {
    "watch": [
      "./**/*",
      "server.ts"
    ],
    "ext": "ts, js, html",
    "exec": "node --loader ts-node/esm --experimental-specifier-resolution=node server.ts"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "joi": "^17.13.3",
    "jsonfile": "^6.1.0",
    "mysql2": "^3.11.3",
    "process": "^0.11.10",
    "sequelize": "^6.37.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.9",
    "eslint": "^9.12.0",
    "eslint-plugin-n": "^17.11.1",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  }
}
